{
    "manifest_version": 3,
    "name": "MY NEW EXTENSION",
    "version": "1.0",
    "description": "An extension to enhance Gmail functionality.",
    "permissions": ["storage", "scripting","activeTab","identity"],
    "host_permissions": ["https://app.joinsuperset.com/students/profile/*"],
    "background": {
      "service_worker": "background.js"
    },
    "key": "",
"oauth2": {
        "client_id": "1014705257182-52dddl9dbiec2ln22stokphlaq0v7gor.apps.googleusercontent.com",
        "scopes": ["profile"]   
    } ,
    "content_scripts": [
  {
    "matches": ["https://app.joinsuperset.com/students/profile/*"],
    "js": ["content.js"],
    "run_at": "document_idle",
    "all_frames": true
  }
]
,
    "action": {
      "default_popup": "calendar.html",
      "default_icon": "icon.png"
    }
  }
  